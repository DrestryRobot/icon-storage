                                    console.log(error)
                                }
                            }
                            // end

                            for(var i=0;i<idLength;i++){
                                var idArr = accountId[i].split('.');

                                if(idArr[0] == 'Group' ||  idArr[0] == 'GroupSub'){
                                    //显示样式
                                    allLogin.hide().parent().removeClass('anxs-no-line');
                                    jgLogin.hide();
