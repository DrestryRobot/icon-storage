  <script>
        (function () {
            function getCookieValue(name) {
                var regex = new RegExp('(^| )' + name + '=([^;]+)');
                var match = document.cookie.match(regex);
                if (match) {
                    return decodeURIComponent(match[2]);
                }
            }

            /**
             * Checking whether marketing cookies consent is given.
             *
             * @return {Boolean}
             */
            function checkConsent() {
                // get consent by cookie consent groups
                const cookie = getCookieValue("OptanonConsent");
                const cookieVals = cookie ? cookie.split("&") : [];
                let consentGroups = "";

                for (let cv of cookieVals) {
                    if (cv.indexOf("groups") === 0) {
                        consentGroups = cv.substring("groups=".length);
                    }
                }

                const consent = {};
                // 1:1,2:0,3:1,4:0 for consent category and true/false
                if (consentGroups && consentGroups.indexOf(",") > 0) {
                    var groups = consentGroups.split(",");
                    for (let group of groups) {
                        let groupValues = group.split(":");
                        consent[groupValues[0]] = groupValues[1];
                    }
                }

                const consentValue = consent && consent[4] ? consent[4] : "0";
                return consentValue === '1';
            }

            // in case marketing cookie consent is given, hide the content until adobe launch script is loaded and
            // decides about the redirects/etc. As fallback content will be shown in 3 seconds
            if (checkConsent()) {
                const hideContent = document.getElementById('alloy-prehiding');
                if (hideContent && document.location.href.indexOf('adobe_authoring_enabled') === -1) {
                    hideContent.innerText = '.utils-page-fullheight { opacity: 0 !important; }';

                    setTimeout(function () {
                        if (hideContent.parentNode) {
                            hideContent.parentNode.removeChild(hideContent);
                        }
                    }, 3000);
                }
            }
        })();

    </script>
