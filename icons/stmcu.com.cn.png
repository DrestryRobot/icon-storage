			obj.onload = null;
			$.ajax({
				url: 'https://www.stmcu.com.cn/Designresource/ajax_load_image ',
				data: {
					img_path: img_path
				},
				type: 'post',
				dataType: 'json',
				//async:false,
				success: function(data) {
					if (data.status == 0) {
						$(obj).attr('src', data.res);
